    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "allure-cypress": "^3.4.5",
    "cypress": "^15.10.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5"
