{
  "name": "imobiliaria",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force && npm run db:seed",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:e2e:ui": "start-server-and-test dev http://localhost:3000 cypress:open",
    "test:e2e:allure": "cypress run --reporter json --allure-results-dir allure-results && allure generate allure-results -o allure-report",
    "test:allure:report": "allure open allure-report"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
    "@neondatabase/serverless": "^1.0.1",
    "@prisma/client": "^6.11.1",
    "@types/bcryptjs": "^2.4.6",
    "@uploadthing/react": "^7.3.2",
    "bcryptjs": "^3.0.2",
    "lucide-react": "^0.525.0",
    "next": "15.3.8",
    "next-auth": "^4.24.11",
    "prisma": "^6.11.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "uploadthing": "^7.7.3",
    "zod": "^4.0.5"
